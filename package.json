{
    "name": "my-features",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "npx ts-node server/index.tsx",
        "dev:watch": "npx ts-node-dev --respawn server/index.tsx",
        "dev": "npx ts-node server/index.tsx",
        "build": "tsc",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public"
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "classnames": "^2.3.1",
        "fastify": "^3.21.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "sass": "^1.39.0",
        "typescript": "^4.1.2",
        "web-vitals": "^1.0.1"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.3.8",
        "@storybook/addon-essentials": "^6.3.8",
        "@storybook/addon-links": "^6.3.8",
        "@storybook/node-logger": "^6.3.8",
        "@storybook/preset-create-react-app": "^3.2.0",
        "@storybook/react": "^6.3.8",
        "ts-node-dev": "^1.1.8"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "volta": {
        "node": "14.17.6"
    }
}
